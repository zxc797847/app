<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal" >


    <LinearLayout
        android:id="@+id/ll_choice"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#EDECEC"
        android:orientation="vertical" >
        
        <LinearLayout
            android:id="@+id/ll_choice11"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >
            
            	<LinearLayout
		            android:id="@+id/ll_choice12"
		            android:layout_width="180dp"
		            android:layout_height="wrap_content"
		            android:orientation="horizontal" >
		            
            	    	<TextView
			                android:id="@+id/tv_BQH_name"
			                android:layout_width="wrap_content"
			                android:layout_height="wrap_content"
			                android:layout_marginLeft="2dp"
			                android:text="标签号："
			                android:textColor="@color/black"
			                android:textSize="10dp" />
			
			            <TextView
			                android:id="@+id/tv_BQH"
			                android:layout_width="130dp"
			                android:layout_height="wrap_content"
			                android:text=""
			                android:textColor="@color/blue"
			                android:textSize="12dp" />
           	    </LinearLayout>

            	<LinearLayout
		            android:id="@+id/ll_choice13"
		            android:layout_width="match_parent"
		            android:layout_height="wrap_content"
		            android:orientation="horizontal" >
		            
            	    	<TextView
			                android:id="@+id/tv_HJH_name"
			                android:layout_width="wrap_content"
			                android:layout_height="wrap_content"
			                android:layout_marginLeft="2dp"
			                android:text="货位号："
			                android:textColor="@color/black"
			                android:textSize="10dp" />
			
			            <TextView
			                android:id="@+id/tv_HJH"
			                android:layout_width="wrap_content"
			                android:layout_height="match_parent"
			                android:text=""
			                android:textColor="@color/blue"
			                android:textSize="10dp" />
           	    </LinearLayout>
        </LinearLayout>
        
        <LinearLayout
            android:id="@+id/ll_choice14"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >
            
            	<LinearLayout
		            android:id="@+id/ll_choice15"
		            android:layout_width="match_parent"
		            android:layout_height="wrap_content"
		            android:orientation="horizontal" >
		            
            	    	<TextView
			                android:id="@+id/tv_CPMC_name"
			                android:layout_width="wrap_content"
			                android:layout_height="wrap_content"
			                android:layout_marginLeft="2dp"
			                android:text="产品名称："
			                android:textColor="@color/black"
			                android:textSize="10dp" />
			
			            <TextView
			                android:id="@+id/tv_CPMC"
			                android:layout_width="wrap_content"
			                android:layout_height="match_parent"
			                android:text=""
			                android:textColor="@color/blue"
			                android:textSize="10dp" />
           	    </LinearLayout>


        </LinearLayout>

		<LinearLayout
			android:id="@+id/ll_choice28"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:orientation="horizontal" >


			<LinearLayout
				android:id="@+id/ll_choice26"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:orientation="horizontal" >

				<TextView
					android:id="@+id/tv_CPXH_name"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginLeft="2dp"
					android:text="产品型号："
					android:textColor="@color/black"
					android:textSize="10dp" />

				<TextView
					android:id="@+id/tv_CPXH"
					android:layout_width="wrap_content"
					android:layout_height="match_parent"
					android:text=""
					android:textColor="@color/blue"
					android:textSize="10dp" />
			</LinearLayout>
		</LinearLayout>
        
        <LinearLayout
            android:id="@+id/ll_choice17"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >
            
            	<LinearLayout
		            android:id="@+id/ll_choice18"
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:orientation="horizontal" >
		            
            	    	<TextView
			                android:id="@+id/tv_CPGG_name"
			                android:layout_width="wrap_content"
			                android:layout_height="wrap_content"
			                android:layout_marginLeft="2dp"
			                android:text="产品规格："
			                android:textColor="@color/black"
			                android:textSize="10dp" />
			
			            <TextView
			                android:id="@+id/tv_CPGG"
			                android:layout_width="wrap_content"
			                android:layout_height="match_parent"
			                android:text=""
			                android:textColor="@color/blue"
			                android:textSize="10dp" />
           	    </LinearLayout>

        </LinearLayout>

		<LinearLayout
			android:id="@+id/ll_choice29"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:orientation="horizontal" >


			<LinearLayout
				android:id="@+id/ll_choice19"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:orientation="horizontal" >

				<TextView
					android:id="@+id/tv_SL_name"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginLeft="2dp"
					android:text="库存数量："
					android:textColor="@color/black"
					android:textSize="10dp" />

				<TextView
					android:id="@+id/tv_SL"
					android:layout_width="100dp"
					android:layout_height="match_parent"
					android:layout_marginRight="8dp"
					android:text=""
					android:textColor="@color/blue"
					android:textSize="10dp" />
			</LinearLayout>

			<LinearLayout
				android:id="@+id/ll_choice21"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:orientation="horizontal" >

				<TextView
					android:id="@+id/tv_LYSL_name"
					android:layout_width="wrap_content"
					android:layout_height="match_parent"
					android:layout_marginLeft="2dp"
					android:text="退回数量："
					android:textColor="@color/black"
					android:textSize="10dp" />

				<EditText
					android:id="@+id/et_LYSL"
					android:layout_width="120dp"
					android:layout_height="match_parent"
					android:layout_marginTop="-12dp"
					android:inputType="number"
					android:text=""
					android:textColor="@color/blue"
					android:textSize="10dp" />
			</LinearLayout>
		</LinearLayout>


        <LinearLayout
            android:id="@+id/ll_choice24"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
			android:layout_marginTop="3dp"
            android:orientation="horizontal" >
            	
            	<LinearLayout
		            android:id="@+id/ll_choice25"
		            android:layout_width="match_parent"
		            android:layout_height="wrap_content"
		            android:orientation="horizontal" >
				         <TextView
			                 android:id="@+id/tv_pro_rfid_search"
			                 android:layout_width="110dp"
			                 android:layout_height="wrap_content"
			                 android:layout_marginBottom="2dp"
			                 android:layout_marginLeft="10dp"
			                 android:layout_marginRight="8dp"
			                 android:background="@drawable/btn_bg_selector"
			                 android:gravity="center"
							 android:paddingTop="2dp"
							 android:paddingBottom="2dp"
			                 android:text="查找"
			                 android:textColor="@drawable/btn_text_selector" 
			                 android:textSize="10dp"/>
			             
			             <TextView
			                 android:id="@+id/tv_pro_info_delete"
			                 android:layout_width="110dp"
			                 android:layout_height="wrap_content"
			                 android:layout_marginBottom="2dp"
			                 android:layout_marginRight="8dp"
			                 android:background="#FF8C00"
			                 android:gravity="center"
							 android:paddingTop="2dp"
							 android:paddingBottom="2dp"
			                 android:text="清除"
			                 android:textColor="@drawable/btn_text_selector" 
			                 android:textSize="10dp"/>
			             
			             <TextView
			                 android:id="@+id/tv_pro_info_save"
			                 android:layout_width="110dp"
			                 android:layout_height="wrap_content"
			                 android:layout_marginBottom="2dp"
			                 android:layout_marginRight="5dp"
			                 android:background="@drawable/btn_bg_selector"
			                 android:gravity="center"
			                 android:paddingTop="2dp"
			                 android:paddingBottom="2dp"
			                 android:text="提交"
			                 android:textColor="@drawable/btn_text_selector" 
			                 android:textSize="10dp"/>
	             </LinearLayout>
          </LinearLayout>
        
        <TextView
	          android:id="@+id/tv_EPC"
	          android:layout_width="314dp"
	          android:layout_height="match_parent"
	          android:layout_marginEnd="16dp"
	          android:layout_marginRight="16dp"
	          android:paddingTop="11dp"
	          android:text=""
	          android:textColor="@color/blue"
	          android:textSize="12dp"
	          android:visibility="gone" />

   	</LinearLayout>
</LinearLayout>