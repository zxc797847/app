<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="horizontal" >


	<LinearLayout
		android:id="@+id/ll_choice"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:background="@drawable/shape_corner_up"
		android:orientation="vertical" >

		<LinearLayout
			android:id="@+id/ll_choice11"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:padding="4dp"
			android:orientation="horizontal" >

			<LinearLayout
				android:id="@+id/ll_choice12"
				android:layout_width="120dp"
				android:layout_height="wrap_content"
				android:orientation="horizontal" >

				<TextView
					android:id="@+id/tv_BQH_name"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginLeft="2dp"
					android:text="标签号："
					android:textColor="@color/black"
					android:textSize="14dp" />

				<TextView
					android:id="@+id/tv_BQH"
					android:layout_width="130dp"
					android:layout_height="wrap_content"
					android:text="100903"
					android:textColor="@color/blue"
					android:textSize="15dp" />
			</LinearLayout>

			<LinearLayout
				android:id="@+id/ll_choice13"
				android:layout_width="140dp"
				android:layout_height="wrap_content"
				android:orientation="horizontal" >

				<TextView
					android:id="@+id/tv_HJH_name"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginLeft="2dp"
					android:text="货位号："
					android:textColor="@color/black"
					android:textSize="14dp" />

				<TextView
					android:id="@+id/tv_HJH"
					android:layout_width="90dp"
					android:layout_height="match_parent"
					android:text="10-08-1-01"
					android:textColor="@color/blue"
					android:textSize="15dp" />
			</LinearLayout>

			<LinearLayout
				android:id="@+id/ll_choice19"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:orientation="horizontal" >

				<TextView
					android:id="@+id/tv_SL_name"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginLeft="2dp"
					android:text="库存数量："
					android:textColor="@color/gray"
					android:textSize="14dp" />

				<TextView
					android:id="@+id/tv_SL"
					android:layout_width="31dp"
					android:layout_height="match_parent"
					android:layout_marginRight="8dp"
					android:text="1"
					android:textColor="@color/gray"
					android:textSize="14dp" />
			</LinearLayout>

		</LinearLayout>

		<LinearLayout
			android:id="@+id/ll_choice14"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:padding="4dp"
			android:orientation="horizontal" >

			<LinearLayout
				android:id="@+id/ll_choice15"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:orientation="horizontal" >

				<TextView
					android:id="@+id/tv_CPMC"
					android:layout_width="wrap_content"
					android:layout_height="match_parent"
					android:text="LMK//合金铝用铣刀//12×D12×75L×30-3F"
					android:textColor="@color/gray"
					android:textSize="15dp" />
			</LinearLayout>


		</LinearLayout>

		<LinearLayout
			android:id="@+id/ll_choice129"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:orientation="horizontal"
			android:padding="4dp">

			<LinearLayout
				android:id="@+id/ll_choice23"
				android:layout_width="360dp"
				android:layout_height="wrap_content"
				android:layout_marginRight="10dp"
				android:orientation="horizontal">

				<TextView
					android:id="@+id/tv_LYSL_name"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginLeft="2dp"
					android:text="退回数量："
					android:textColor="@color/black"
					android:textSize="10dp" />

				<EditText
					android:id="@+id/et_LYSL"
					android:layout_width="120dp"
					android:layout_height="36dp"
					android:layout_marginTop="3dp"
					android:inputType="number"
					android:text=""
					android:textColor="@color/blue"
					android:textSize="10dp" />


			</LinearLayout>

		</LinearLayout>

		<LinearLayout
			android:id="@+id/ll_choice24"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:padding="4dp"
			android:orientation="horizontal" >

			<LinearLayout
				android:id="@+id/ll_choice25"
				android:layout_width="match_parent"
				android:layout_height="30dp"
				android:orientation="horizontal" >

				<TextView
					android:id="@+id/tv_pro_rfid_search"
					android:layout_width="110dp"
					android:layout_height="match_parent"
					android:layout_marginLeft="10dp"
					android:layout_marginRight="8dp"
					android:layout_marginBottom="2dp"
					android:background="@drawable/btn_bg_selector"
					android:gravity="center"
					android:paddingTop="2dp"
					android:paddingBottom="2dp"
					android:text="查找"
					android:textColor="@drawable/btn_text_selector"
					android:textSize="10dp" />

				<TextView
					android:id="@+id/tv_pro_info_delete"
					android:layout_width="110dp"
					android:layout_height="30dp"
					android:layout_marginRight="8dp"
					android:layout_marginBottom="2dp"
					android:background="@drawable/btn_bg_delete"
					android:gravity="center"
					android:paddingTop="2dp"
					android:paddingBottom="2dp"
					android:text="清除"
					android:textColor="@drawable/btn_text_selector"
					android:textSize="10dp" />

				<TextView
					android:id="@+id/tv_pro_info_save"
					android:layout_width="110dp"
					android:layout_height="30dp"
					android:layout_marginRight="5dp"
					android:layout_marginBottom="2dp"
					android:background="@drawable/btn_bg_selector"
					android:gravity="center"
					android:paddingTop="2dp"
					android:paddingBottom="2dp"
					android:text="提交"
					android:textColor="@drawable/btn_text_selector"
					android:textSize="10dp" />
			</LinearLayout>
		</LinearLayout>

		<TextView
			android:id="@+id/tv_EPC"
			android:layout_width="314dp"
			android:layout_height="match_parent"
			android:layout_marginEnd="16dp"
			android:layout_marginRight="16dp"
			android:paddingTop="11dp"
			android:text=""
			android:textColor="@color/blue"
			android:textSize="12dp"
			android:visibility="gone" />
		<TextView
			android:id="@+id/tv_position"
			android:layout_width="314dp"
			android:layout_height="match_parent"
			android:layout_marginEnd="16dp"
			android:layout_marginRight="16dp"
			android:paddingTop="11dp"
			android:text=""
			android:textColor="@color/blue"
			android:textSize="12dp"
			android:visibility="gone" />

	</LinearLayout>
</LinearLayout>