<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:background="#FFFFFF" >


	<LinearLayout
		android:id="@+id/ll_choice"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:background="#EDECEC"
		android:orientation="vertical"
		android:paddingBottom="1dp" >

		<LinearLayout
			android:id="@+id/ll_pro_rfid_mgr_shelf_search_head"
			android:layout_width="match_parent"
			android:layout_height="28dp"
			android:background="#2fa4e7"
			android:orientation="horizontal" >

			<ImageView
				android:id="@+id/iv_back"
				android:layout_width="0dp"
				android:layout_height="match_parent"
				android:layout_weight="1"
				android:padding="8dp"
				android:src="@drawable/tb_icon_actionbar_back" />

			<TextView
				android:id="@+id/tv_top_title"
				android:layout_width="0dp"
				android:layout_height="match_parent"
				android:layout_weight="4"
				android:gravity="center"
				android:text="领用退回入库"
				android:textColor="#FFFFFF"
				android:textSize="20dp" />

			<ImageView
				android:id="@+id/iv_pro_inventory_mgr_inventory_more"
				android:layout_width="0dp"
				android:layout_height="match_parent"
				android:layout_weight="1"
				android:padding="8dp"
				android:src="@drawable/tb_icon_actionbar_more" />
		</LinearLayout>

		<LinearLayout
			android:id="@+id/ll_choice2"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:background="#EDECEC"
			android:orientation="vertical" >

			<LinearLayout
				android:id="@+id/ll_choice9"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:background="#EDECEC"
				android:orientation="vertical" >

				<LinearLayout
					android:id="@+id/ll_choice32"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginLeft="2dp"
					android:focusable="true"
					android:focusableInTouchMode="true"
					android:gravity="left" >

					<TextView
						android:id="@+id/tv_BQH_NAME"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_marginLeft="10dp"
						android:text="标签号："
						android:textColor="@color/black"
						android:textSize="14dp" />

					<EditText
						android:id="@+id/tv_BQH_VAL"
						android:layout_width="80dp"
						android:layout_height="wrap_content"
						android:layout_marginRight="1dp"
						android:textSize="14dp"/>

					<TextView
						android:id="@+id/tv_pro_bqh_search"
						android:layout_width="80dp"
						android:layout_height="wrap_content"
						android:background="@drawable/btn_bg_selector"
						android:gravity="center"
						android:paddingTop="2dp"
						android:paddingBottom="2dp"
						android:layout_marginRight="2dp"
						android:text="输入卡号查询"
						android:textColor="@drawable/btn_text_selector"
						android:textSize="12dp"/>

					<TextView
						android:id="@+id/tv_pro_read_card_search"
						android:layout_width="60dp"
						android:layout_height="wrap_content"
						android:background="@drawable/btn_bg_selector"
						android:gravity="center"
						android:paddingTop="2dp"
						android:paddingBottom="2dp"
						android:layout_marginRight="2dp"
						android:text="读卡查询"
						android:textColor="@drawable/btn_text_selector"
						android:textSize="12dp"/>

					<TextView
						android:id="@+id/tv_pro_rfid_search"
						android:layout_width="50dp"
						android:layout_height="wrap_content"
						android:background="@drawable/btn_bg_selector"
						android:gravity="center"
						android:paddingTop="2dp"
						android:paddingBottom="2dp"
						android:layout_marginRight="2dp"
						android:text="查询"
						android:textColor="@drawable/btn_text_selector"
						android:textSize="12dp"/>

					<TextView
						android:id="@+id/tv_pro_info_delete"
						android:layout_width="50dp"
						android:layout_height="wrap_content"
						android:background="@drawable/btn_bg_delete"
						android:gravity="center"
						android:paddingTop="2dp"
						android:paddingBottom="2dp"
						android:text="清除"
						android:textColor="@drawable/btn_text_selector"
						android:textSize="12dp"/>

				</LinearLayout>

				<LinearLayout
					android:id="@+id/ll_choice3"
					android:layout_width="match_parent"
					android:layout_height="24dp"
					android:layout_marginLeft="2dp"
					android:focusable="true"
					android:focusableInTouchMode="true"
					android:gravity="left" >

					<TextView
						android:id="@+id/tv_ckmc_name"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_marginLeft="10dp"
						android:text="仓库名称："
						android:textColor="@color/black"
						android:textSize="14dp" />

					<Spinner
						android:id="@+id/spinner_ckmc"
						android:layout_width="100dp"
						android:layout_height="wrap_content"
						android:layout_marginRight="1dp" />



				</LinearLayout>

				<LinearLayout
					android:id="@+id/ll_choice322"
					android:layout_width="match_parent"
					android:layout_height="24dp"
					android:layout_marginLeft="2dp"
					android:focusable="true"
					android:focusableInTouchMode="true"
					android:gravity="left" >

					<TextView
						android:id="@+id/tv_hjh_name"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_marginLeft="10dp"
						android:text="货  位  号："
						android:textColor="@color/black"
						android:textSize="14dp" />

					<Spinner
						android:id="@+id/spinner_hjh"
						android:layout_width="320dp"
						android:layout_height="wrap_content"
						android:layout_marginRight="1dp" />

				</LinearLayout>

				<LinearLayout
					android:id="@+id/ll_choice4"
					android:layout_width="match_parent"
					android:layout_height="24dp"
					android:layout_marginLeft="2dp"
					android:focusable="true"
					android:focusableInTouchMode="true"
					android:gravity="left" >

					<TextView
						android:id="@+id/tv_cpmc_name"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_marginLeft="10dp"
						android:text="产品名称："
						android:textColor="@color/black"
						android:textSize="14dp" />

					<Spinner
						android:id="@+id/spinner_cpmc"
						android:layout_width="320dp"
						android:layout_height="match_parent"
						android:layout_marginRight="1dp"/>

				</LinearLayout>

				<LinearLayout
					android:id="@+id/ll_choice5"
					android:layout_width="match_parent"
					android:layout_height="24dp"
					android:layout_marginLeft="2dp"
					android:focusable="true"
					android:focusableInTouchMode="true"
					android:gravity="left" >

					<TextView
						android:id="@+id/tv_cpxh_name"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_marginLeft="10dp"
						android:text="产品型号："
						android:textColor="@color/black"
						android:textSize="14dp" />

					<Spinner
						android:id="@+id/spinner_cpxh"
						android:layout_width="320dp"
						android:layout_height="match_parent"
						android:layout_marginRight="1dp"/>

				</LinearLayout>

				<LinearLayout
					android:id="@+id/ll_choice6"
					android:layout_width="match_parent"
					android:layout_height="24dp"
					android:layout_marginLeft="2dp"
					android:focusable="true"
					android:focusableInTouchMode="true"
					android:gravity="left" >

					<TextView
						android:id="@+id/tv_cpgg_name"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_marginLeft="10dp"
						android:text="产品规格："
						android:textColor="@color/black"
						android:textSize="14dp" />

					<Spinner
						android:id="@+id/spinner_cpgg"
						android:layout_width="320dp"
						android:layout_height="match_parent"
						android:layout_marginRight="1dp"/>

				</LinearLayout>
				<!--<LinearLayout
                    android:id="@+id/ll_choice10"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:layout_marginLeft="2dp"
                    android:layout_marginTop="16dp"
                    android:focusable="true"
                    android:focusableInTouchMode="true"
                    android:gravity="left" >
                    <TextView
                            android:id="@+id/tv_pro_rfid_search"
                            android:layout_width="180dp"
                            android:layout_height="wrap_content"
