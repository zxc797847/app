<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF" >


    <LinearLayout
        android:id="@+id/ll_choice"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#EDECEC"
        android:orientation="vertical"
        android:paddingBottom="1dp" >

        <LinearLayout
            android:id="@+id/ll_pro_rfid_mgr_shelf_search_head"
            android:layout_width="match_parent"
            android:layout_height="28dp"
            android:background="#2fa4e7"
            android:orientation="horizontal" >

            <ImageView
                android:id="@+id/iv_back"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:padding="8dp"
                android:src="@drawable/tb_icon_actionbar_back" />

            <TextView
                android:id="@+id/tv_top_title"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="4"
                android:gravity="center"
                android:text="领用查询"
                android:textColor="#FFFFFF"
                android:textSize="20dp" />

            <ImageView
                android:id="@+id/iv_pro_inventory_mgr_inventory_more"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:padding="8dp"
                android:src="@drawable/tb_icon_actionbar_more" />
        </LinearLayout>

		<!--<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_marginTop="30dp"
			android:orientation="horizontal"
			android:gravity="center">

			<Button
				android:id="@+id/btTime"
				android:layout_width="150dp"
				android:layout_height="50dp"
				android:text="@string/time"
				android:textSize="22sp"
				android:textColor="#ffffff"
				android:background="@drawable/button_green_bg"/>


		</LinearLayout>-->

        <LinearLayout
            android:id="@+id/ll_choice2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#EDECEC"
            android:orientation="vertical"
            android:paddingBottom="2dp"
            android:paddingTop="2dp" >
            
            <LinearLayout
	            android:id="@+id/ll_choice9"
	            android:layout_width="match_parent"
	            android:layout_height="wrap_content"
	            android:background="#EDECEC"
	            android:orientation="vertical"
	            android:paddingBottom="2dp"
	            android:paddingTop="2dp" >

                <LinearLayout
                    android:id="@+id/ll_choice32"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="2dp"
                    android:layout_marginTop="2dp"
                    android:focusable="true"
                    android:focusableInTouchMode="true"
                    android:gravity="left" >

                    <TextView
					android:id="@+id/tv_BQH_NAME"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginLeft="10dp"
					android:text="日期："
					android:textColor="@color/black"
					android:textSize="18dp" />

					<EditText
						android:id="@+id/et_KSRQ"
						android:layout_width="90dp"
						android:layout_height="wrap_content"
						android:background="@color/help_button_view"
						android:layout_marginRight="1dp"
						android:clickable="false"
						android:enabled="false"
						android:focusable="false"
						android:textSize="18dp"/>

					<Button
						android:id="@+id/btCalender"
						android:layout_width="50dp"
						android:layout_height="25dp"
						android:layout_marginLeft="2dp"
						android:layout_marginTop="-2dp"
						android:text="点击"
						android:textSize="12dp"
						android:textColor="#ffffff"
						android:background="@drawable/btn_bg_selector"/>

					<TextView
						android:id="@+id/tv_to"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_marginLeft="8dp"
                        android:layout_marginRight="8dp"
						android:text="到"
						android:textColor="@color/black"
						android:textSize="18dp" />

					<EditText
						android:id="@+id/et_JSRQ"
						android:layout_width="90dp"
						android:layout_height="wrap_content"
						android:background="@color/help_button_view"
						android:clickable="false"
						android:enabled="false"
						android:focusable="false"
						android:textSize="18dp"/>

					<Button
						android:id="@+id/btCalender1"
						android:layout_width="50dp"
						android:layout_height="25dp"
						android:layout_marginLeft="2dp"
						android:layout_marginTop="-2dp"
						android:text="点击"
						android:textSize="12dp"
						android:textColor="#ffffff"
						android:background="@drawable/btn_bg_selector"/>

                </LinearLayout>

	            <LinearLayout
	                android:id="@+id/ll_choice3"
	                android:layout_width="match_parent"
	                android:layout_height="25dp"
	                android:layout_marginLeft="2dp"
	                android:layout_marginTop="8dp"
	                android:focusable="true"
	                android:focusableInTouchMode="true"
	                android:gravity="left" >

	                <TextView
	                    android:id="@+id/tv_cpbm_name"
	                    android:layout_width="wrap_content"
	                    android:layout_height="wrap_content"
	                    android:layout_marginLeft="10dp"
	                    android:text="产品编码："
	                    android:textColor="@color/black"
	                    android:textSize="18dp" />

					<EditText
						android:id="@+id/et_cpbm"
						android:layout_width="260dp"
						android:layout_height="wrap_content"
						android:background="@color/help_button_view"
						android:layout_marginRight="1dp"
						android:textSize="18dp"/>
	
	                
	            </LinearLayout>

	            <LinearLayout
	                android:id="@+id/ll_choice10"
	                android:layout_width="match_parent"
	                android:layout_height="40dp"
	                android:layout_marginLeft="2dp"
	                android:layout_marginTop="16dp"
	                android:focusable="true"
	                android:focusableInTouchMode="true"
	                android:gravity="left" >
		            <TextView
		                    android:id="@+id/tv_pro_rfid_search"
		                    android:layout_width="180dp"
		                    android:layout_height="wrap_content"
		                    android:layout_marginBottom="2dp"
		                    android:layout_marginRight="18dp"
		                    android:background="@drawable/btn_bg_selector"
		                    android:gravity="center"
		                    android:paddingTop="8dp"
		                    android:paddingBottom="8dp"
		                    android:text="查询"
		                    android:textColor="@drawable/btn_text_selector" 
		                    android:textSize="12dp"/>
		                
		                <TextView
		                    android:id="@+id/tv_pro_info_delete"
		                    android:layout_width="180dp"
		                    android:layout_height="wrap_content"
		                    android:layout_marginBottom="2dp"
		                    android:background="@drawable/btn_bg_delete"
		                    android:gravity="center"
		                    android:paddingTop="8dp"
		                    android:paddingBottom="8dp"
		                    android:text="清除"
		                    android:textColor="@drawable/btn_text_selector" 
		                    android:textSize="12dp"/>
	                </LinearLayout>
            </LinearLayout>
            
        </LinearLayout>
    </LinearLayout>

    <com.avic.demo.sebeiglzx.utils.scroller.WMSScrollView
        android:id="@+id/wMSScrollView1"
        android:layout_width="400dp"
        android:layout_height="4000dp"
        android:layout_marginTop="150dp" >

        <HorizontalScrollView
            android:layout_width="match_parent"
            android:layout_height="4000dp">

            <RelativeLayout
                 android:layout_width="match_parent"
                 android:layout_height="4000dp"
                 android:layout_marginTop="2dp"
                 android:layout_weight="0.20" >


				<View
					android:layout_width="match_parent"
					android:layout_height="1dip"
					android:background="#A7A5A5" />
				<!-- 调用外部控件的布局list_item布局。 -->

				<include
					android:id="@+id/table_title"
					layout="@layout/list_item" />

				<View
					android:layout_width="match_parent"
					android:layout_height="1.5dip"
					android:background="#686867" />

				<!--<ListView
					android:id="@+id/listView_fkb_info"
					android:layout_width="400dp"
					android:layout_height="130dp"
					android:layout_marginTop="36dp"
					android:cacheColorHint="@android:color/transparent"
					android:divider="#B4B3B3"
					android:dividerHeight="1.0dip"
					android:fadingEdge="none"
					android:scrollbars="none" >
				</ListView>-->

				<ListView
					android:id="@+id/listView_fkb_info"
					android:layout_width="388dp"
					android:layout_height="4000dp"
					android:layout_alignParentLeft="true"
					android:layout_marginLeft="2dp"
					android:layout_marginTop="38dp"
					android:divider="@color/silver"
					android:dividerHeight="3px"
					android:paddingBottom="50dp" >
				</ListView>
             </RelativeLayout>
        </HorizontalScrollView>
    </com.avic.demo.sebeiglzx.utils.scroller.WMSScrollView>

</RelativeLayout>