<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF" >


    <LinearLayout
        android:id="@+id/ll_choice"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#EDECEC"
        android:orientation="vertical" >

		<LinearLayout
			android:id="@+id/ll_pro_rfid_mgr_shelf_search_head"
			android:layout_width="match_parent"
			android:layout_height="28dp"
			android:background="#2fa4e7"
			android:orientation="horizontal" >

			<ImageView
				android:id="@+id/iv_back"
				android:layout_width="0dp"
				android:layout_height="match_parent"
				android:layout_weight="1"
				android:padding="8dp"
				android:src="@drawable/tb_icon_actionbar_back" />

			<TextView
				android:id="@+id/tv_top_title"
				android:layout_width="0dp"
				android:layout_height="match_parent"
				android:layout_weight="4"
				android:gravity="center"
				android:text="货位调整"
				android:textColor="#FFFFFF"
				android:textSize="20dp" />

			<ImageView
				android:id="@+id/iv_pro_inventory_mgr_inventory_more"
				android:layout_width="0dp"
				android:layout_height="match_parent"
				android:layout_weight="1"
				android:padding="8dp"
				android:src="@drawable/tb_icon_actionbar_more" />
		</LinearLayout>

        <LinearLayout
            android:id="@+id/ll_choice2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#EDECEC"
            android:orientation="vertical"
            android:paddingBottom="1dp"
            android:paddingTop="2dp" >
            
            <LinearLayout
                 android:id="@+id/ll_choice32"
                 android:layout_width="match_parent"
                 android:layout_height="wrap_content"
                 android:layout_marginLeft="2dp"
                 android:layout_marginTop="1dp"
                 android:focusable="true"
                 android:focusableInTouchMode="true"
                 android:gravity="left" >

				 <TextView
					android:id="@+id/tv_pro_read_card_delete"
					android:layout_width="160dp"
					android:layout_height="wrap_content"
					android:layout_marginLeft="20dp"
					android:layout_marginBottom="1dp"
					android:background="@drawable/btn_bg_delete"
					android:gravity="center"
					android:paddingTop="8dp"
					android:paddingBottom="8dp"
					android:text="清除"
					android:textColor="@drawable/btn_text_selector"
					android:textSize="12dp"/>
				 
				 <TextView
					android:id="@+id/tv_pro_read_card_search"
					android:layout_width="160dp"
					android:layout_height="wrap_content"
					android:layout_marginLeft="50dp"
					android:layout_marginBottom="1dp"
					android:background="@drawable/btn_bg_selector"
					android:gravity="center"
					android:paddingTop="8dp"
					android:paddingBottom="8dp"
					android:text="读卡添加"
					android:textColor="@drawable/btn_text_selector"
					android:textSize="12dp"/>

             </LinearLayout>
            
        </LinearLayout>
    
	<LinearLayout
            android:id="@+id/ll_choice198"
            android:layout_width="match_parent"
            android:orientation="horizontal" 
            android:layout_height="416dp"
			android:background="@color/white">
            
	    <com.avic.demo.sebeiglzx.utils.scroller.WMSScrollView
	        android:id="@+id/wMSScrollView1"
	        android:layout_width="match_parent"
	        android:layout_height="12000dp"
	        android:layout_marginTop="0dp" >
	
	        <HorizontalScrollView
	            android:layout_width="match_parent"
	            android:layout_height="12000dp">
	
	            <RelativeLayout
	                 android:layout_width="match_parent"
	                 android:layout_height="12000dp"
	                 android:layout_weight="0.20" >
	
	                 <ListView
				        android:id="@+id/listView_fkb_info"
				        android:layout_width="466dp"
				        android:layout_height="12000dp"
				        android:layout_alignParentLeft="true"
				        android:layout_marginLeft="2dp"
				        android:dividerHeight="3px"
				        android:paddingBottom="50dp" >
				    </ListView>
	             </RelativeLayout>
	        </HorizontalScrollView>
	    </com.avic.demo.sebeiglzx.utils.scroller.WMSScrollView>
	</LinearLayout>
	    
    <LinearLayout
            android:id="@+id/ll_choice128"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >
            
            	<LinearLayout
		            android:id="@+id/ll_choice21"
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:orientation="horizontal" >
		            
            	    	<TextView
	                    android:id="@+id/tv_NEW_HJH_name"
	                    android:layout_width="wrap_content"
	                    android:layout_height="wrap_content"
	                    android:layout_marginLeft="10dp"
	                    android:text="移入货位号："
	                    android:textColor="@color/black"
	                    android:textSize="14dp" />
	                    
	                <EditText
	                    android:id="@+id/et_NEW_HJH"
	                    android:layout_width="200dp"
	                    android:layout_height="28dp"
	                    android:layout_weight="0.03"
	                    android:background="@color/help_button_view"
	                    android:hint="请扫描货位号"
	                    android:singleLine="true"
	                    android:textSize="14dp" />
				    
	                <TextView
	                    android:id="@+id/tv_init_hjh_scan"
	                    android:layout_width="109dp"
	                    android:layout_height="wrap_content"
	                    android:layout_marginLeft="10dp"
	                    android:layout_marginRight="0dp"
	                    android:layout_marginTop="-2dp"
	                    android:layout_weight="0.41"
	                    android:background="@drawable/btn_bg_selector"
	                    android:gravity="center"
	                    android:padding="6dp"
	                    android:text="扫描"
	                    android:textColor="@drawable/btn_text_selector"
	                    android:textSize="12dp" />
           	    </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_choice24"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:orientation="horizontal" >
            	
            	<LinearLayout
		            android:id="@+id/ll_choice25"
		            android:layout_width="match_parent"
		            android:layout_height="wrap_content"
		            android:orientation="horizontal" >
			             
			             <TextView
			                 android:id="@+id/tv_pro_info_save"
			                 android:layout_width="360dp"
			                 android:layout_height="wrap_content"
			                 android:layout_marginBottom="2dp"
			                 android:layout_marginLeft="30dp"
			                 android:layout_marginRight="5dp"
			                 android:background="@drawable/btn_bg_selector"
			                 android:gravity="center"
			                 android:paddingTop="8dp"
			                 android:paddingBottom="8dp"
			                 android:text="提交"
			                 android:textColor="@drawable/btn_text_selector" 
			                 android:textSize="12dp"/>
	             </LinearLayout>
          </LinearLayout>
          
        
	</LinearLayout>
</RelativeLayout>